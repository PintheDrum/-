  m_Name: Scene3.15
  m_EditorClassIdentifier: 
  dialogueLines:
  - speaker: 
    dialogue: "\u4F60\u7F13\u7F13\u6253\u5F00\u95E8, \u53EA\u770B\u89C1\u8212\u68A8\u8138\u8272\u82CD\u767D\u5730\u7AD9\u5728\u4F60\u5BB6\u95E8\u53E3......"
  - speaker: "\u4F60:"
    dialogue: "\u8212\u68A8, \u4F60\u600E\u4E48\u4E86? \u8EAB\u4F53\u4E0D\u8212\u670D\u5417?
      \u8981\u4E0D\u8FDB\u6765\u5750\u5750?"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u4E0D\u7528\u4E86, \u542C\u6211\u8BF4, \u4F60\u4E0D\u8981\u518D\u53BB\u5403\u90A3\u5BB6\u9910\u9986\u4E86!"
  - speaker: "\u4F60:"
    dialogue: "\u4EC0\u4E48? \u4F60\u8BF4\u5B89\u5E86\u5BB6\u5E38\u83DC\u5417?"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u662F\u7684, \u4F60\u4E0D\u8981\u518D\u53BB\u5403\u4E86."
  - speaker: 
    dialogue: "\u4F60\u89C1\u5979\u865A\u5F31\u5F97\u51E0\u4E4E\u65E0\u6CD5\u7AD9\u7A33,
      \u4FBF\u60F3\u8981\u5E26\u5979\u53BB\u533B\u9662."
  - speaker: "\u4F60:"
    dialogue: "\u4F60\u8981\u6CE8\u610F\u8EAB\u4F53\u554A! \u6709\u4EC0\u4E48\u4E8B\u5C31\u76F4\u63A5\u6765\u627E\u6211......"
  - speaker: 
    dialogue: "\u8FD8\u6CA1\u7B49\u4F60\u8BF4\u5B8C, \u5979\u5C31\u6446\u4E86\u6446\u624B,
      \u8F7B\u58F0\u8BF4\u4E86\u53E5\u4EC0\u4E48\u4FBF\u8F6C\u8EAB\u4FBF\u79BB\u53BB\u4E86......"
  - speaker: 
    dialogue: "\u4F60\u770B\u7740\u8212\u68A8\u79BB\u53BB\u7684\u80CC\u5F71\u4F3C\u4E4E\u53D8\u5F97\u8D8A\u6765\u8D8A\u865A\u6D6E,
      \u672C\u60F3\u8FFD\u4E0A\u53BB\u7684\u6B65\u4F10\u4E5F\u968F\u7740\u5FC3\u7684\u8DF3\u52A8\u6C89\u91CD\u8D77\u6765......"
  - speaker: 
    dialogue: "\u76F4\u5230\u697C\u9053\u518D\u6B21\u53D8\u5F97\u7A7A\u8361\u8361,
      \u8212\u68A8\u7684\u5FAE\u5F31\u7684\u58F0\u97F3\u624D\u4F20\u5230\u4F60\u7684\u8033\u4E2D......"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u518D\u89C1\u4E86......\u518D\u89C1\u4E86......"
  - speaker: 
    dialogue: "\u4F60\u5F00\u59CB\u5BB3\u6015\u8D77\u6765, \u4E5F\u4E0D\u592A\u660E\u767D\u4E3A\u4EC0\u4E48\u8212\u68A8\u8BA9\u4F60\u4E0D\u8981\u518D\u53BB\u5B89\u5E86\u5BB6\u5E38\u83DC\u5403\u996D......"
