  dialogueLines:
  - speaker: 
    dialogue: "\u4F60\u521A\u51C6\u5907\u6572\u54CD302\u7684\u95E8, \u5374\u53D1\u73B0\u8001\u540C\u5B66\u8212\u68A8\u6B63\u4ECE\u91CC\u9762\u8D70\u51FA\u6765......"
  - speaker: "\u4F60:"
    dialogue: "\u8212\u68A8, \u4F60\u600E\u4E48\u5728\u8FD9?"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u554A? \u6211\u73B0\u5728\u5C31\u4F4F\u5728\u8FD9\u91CC......"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u4F60\u4E5F\u4F4F\u5728\u8FD9\u680B\u5417? \u771F\u662F\u597D\u4E45\u4E0D\u89C1\u4E86!"
  - speaker: "\u4F60:"
    dialogue: "\u662F\u554A! \u9AD8\u4E2D\u6BD5\u4E1A\u540E\u5C31\u6CA1\u89C1\u8FC7\u4E86,
      \u4F60\u8FD8\u80FD\u8BA4\u5F97\u6211\u771F\u662F\u592A\u597D\u4E86!"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u54C8\u54C8! \u6211\u8FD8\u8BB0\u5F97\u90A3\u65F6\u5019\u4F60\u501F\u6211\u770B\u7684\u6F2B\u753B\u4E66\u8FD8\u88AB\u8001\u5E08\u6CA1\u6536\u4E86!"
  - speaker: "\u4F60:"
    dialogue: "\u54C8\u54C8! \u4F60\u8D54\u7ED9\u6211\u7684\u90A3\u672C\u6F2B\u753B\u6211\u8FD8\u7559\u7740\u5462!"
  - speaker: "\u4F60:"
    dialogue: "\u4F60\u6709\u4E8B\u8981\u51FA\u53BB\u5417?"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u6CA1\u4EC0\u4E48\u8981\u7D27\u7684\u4E8B, \u6211\u5C31\u53BB\u516C\u56ED\u6563\u6563\u6B65.
      \u4F60\u8981\u4E00\u8D77\u5417?"
  - speaker: "\u4F60:"
    dialogue: "\u4ECA\u5929\u52A0\u73ED\u5B9E\u5728\u662F\u592A\u7D2F\u4E86! \u660E\u5929\u8981\u4E0D\u53EF\u4EE5\u4E00\u8D77?"
  - speaker: "\u8212\u68A8:"
    dialogue: "\u597D\u5440! \u660E\u5929\u6211\u4E0A\u53BB\u53EB\u4F60!"
  - speaker: 
    dialogue: "\u5728\u4E00\u756A\u8C08\u8BBA\u540E, \u4F60\u4E0E\u8212\u68A8\u7EA6\u5B9A\u4E86\u660E\u5929\u53BB\u516C\u56ED\u6563\u6B65......"
